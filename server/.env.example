# Copy to .env and fill with your secrets
PORT=8080
# Supabase service role key (server-only!)
SUPABASE_SERVICE_ROLE=your_service_role_key_here
# Supabase project URL
SUPABASE_URL=https://your-project.supabase.co
# Optional: Postgres connection string (if you prefer direct pg transactions)
# If provided, transactions will use this; otherwise, Supabase service_role client is used for writes.
DATABASE_URL=postgres://user:password@host:5432/database
# NODE_ENV=development or production
NODE_ENV=development
